<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>All Movies</h1>
      <ul class="list-group mb-3">
        <% @lists.each do |list| %>
          <li class="list-group-item">
            <%= List.name%>
          </li>
        <% end %>
      </ul>
       <%= link_to 'show', list_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
